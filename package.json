{
  "name": "express-load-balancer",
  "version": "1.0.0",
  "description": "Aplicação para balancer a carga de requisições da aplicaçãi btm-api",
  "main": "index.js",
  "repository": "git@gitlab.com:r.maycon/express-load-balancer.git",
  "author": "Maycon <mayconrebordao2014@gmail.com>",
  "license": "MIT",
  "scripts": {
    "build": "babel --presets minify index.js --out-dir lib",
    "dev": "nodemon --config nodemon.json  --exec node ./index.js --dev",
    "start-dev": "./node_modules/nodemon/bin/nodemon.js --config nodemon.json --exec node ./index.js --dev local ",
    "start": "node ./index.js",
    "start-s": "node ./lib/index.js",
    "restart": "touch ./index.js"
  },
  "private": true,
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-minify": "^0.4.3",
    "chalk": "^2.4.1",
    "chokidar": "^2.0.4",
    "express": "^4.16.3",
    "gulp-nodemon": "^2.2.1",
    "mongoose": "^5.1.7",
    "node-port-check": "^2.0.0",
    "nodemon": "^1.17.5",
    "normalize-url": "^3.0.1",
    "npm-run-all": "^4.1.3",
    "request": "^2.87.0",
    "run-all": "^1.0.1"
  },
  "devDependencies": {
    "minimist": "^1.2.0"
  }
}
